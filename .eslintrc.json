{
    "root": true,
    "env": {
        "browser": true,
        "es2017": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "plugins": [
        "html",
        "prettier"
    ],
    "rules": {
        "quotes":[
            "error",
            "backtick"
        ],
        "prettier/prettier": "error"
    }
}
